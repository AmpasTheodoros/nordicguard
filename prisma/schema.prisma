generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BackgroundCheck {
  id             String   @id @default(cuid())
  status         String
  name           String
  personalNumber String
  result         Json?
  initiatedBy    String
  initiatedAt    DateTime @default(now())
  completedAt    DateTime?
}
